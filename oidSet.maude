omod OID-SET is 
    protecting STRING .

    sort OidSet .

    subsort String < Oid < OidSet .
    
    op none : -> OidSet [ctor] .
    op __ : OidSet OidSet -> OidSet [ctor assoc comm id: none] .
    op _\_ : OidSet OidSet -> OidSet [assoc] .

    vars OS OS2 : OidSet .
    var O : Oid .
    
    eq OS \ none = OS .
    eq none \ OS = none .
    eq (O OS) \ (O OS2) = OS \ OS2 .
endom